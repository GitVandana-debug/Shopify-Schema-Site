      "settings": [
        {
          "type": "text",
          "id": "complete_vaginal_text",
          "label": "Complete Vaginal text"
        },
      ]
     }
    ],       
   "presets": [
                {
                  "category": "Complete Vaginal Part",
                  "name": "Complete Vaginal Section"
                } 
             ]
  }
{% endschema %}