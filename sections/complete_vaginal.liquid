
   <div class="">
           
    <div class="hero-section">
        <div class="container bi-col">
            <div class="left-col">
                
                <img src="{{ section.settings.lovebug_product_image | img_url: '' }}" alt="LoveBug Product" id="prod-img">
                <div class="list  d-md-none">
                   {% for block in section.blocks %}
                      <span class="icon-heart">
                          {{ block.settings.complete_vaginal_text }}
                      </span>
                   {% endfor %}
                  {% comment %}
                    <span class="icon-heart">
                        Relieve Occasional Odor & Discharge
                    </span>
                    <span class="icon-heart">
                        Lessen Bacterial Vaginitis Recurrences
                    </span>
                    <span class="icon-heart">
                        Lessen UTI Recurrences
                    </span>
                    {% endcomment %}
                </div>
                <a href="{{ section.settings.buy_now_url }}"
                    style="position: relative;
    z-index: 2;">
                    <button class="lg fl  d-md-none">{{ section.settings.buy_now }}</button>
                </a>
                <a href="{{ section.settings.testimonial_url }}" class="testimonial-hero d-md-none hash-link">{{ section.settings.testimonials }}</a>
            </div>
            <div class="right-col condensed">
                <h1 class="heading-sm">
                    <img src="{{ section.settings.made_in_us_image | img_url: '' }}">
                    {{ section.settings.complete_text }}
                    <br />
                    {{ section.settings.urinary_tract }}
                    <br />
                </h1>
                <p class="subtitle">
                    {{ section.settings.text_1 }}
                </p>
                <p class="large-text">
                   {{ section.settings.text_2 }}
                    <br />
                </p>
                <div class="list d-md-grid">
                  {% for block in section.blocks %}
                    <span class="icon-heart">
                        {{ block.settings.complete_vaginal_text }}
                    </span>
                  {% endfor %}
                  {% comment %}
                    <span class="icon-heart">
                        Relieve Occasional Odor & Discharge
                    </span>
                    <span class="icon-heart">
                        Lessen Bacterial Vaginitis Recurrences
                    </span>
                    <span class="icon-heart">
                        Lessen UTI Recurrences
                    </span>
                  {% endcomment %}
                </div>
                <a href="{{ section.settings.buy_now_url }}">
                    <button class="lg fl d-md-block">{{ section.settings.buy_now }}</button>
                </a>
            </div>
        </div>
        <div class="hash-target" id="vaginal-health"></div>
        <div class="main-video">
        </div>
        <br />
        <p class="txt-center cap small-title center">
            {{ section.settings.para_text_1 }}
        </p>
        <div class="container  p-rel">
            <div class="aaogf-badge center">
                <img src="{{ section.settings.aaogf_badge_image | img_url: '' }}">
                <div>
                    <p class="title">
                       {{ section.settings.para_text_2 }}
                    </p>
                    <p class="footnote">
                        {{ section.settings.para_text_3 }}
                    </p>
                </div>
            </div>
        </div>
    </div>
  </div>



{% schema %}
  {
    "name": "Complete Vaginal Part",
    "settings": [
        {
          "type": "image_picker",
          "id": "lovebug_product_image",
